n,d=map(int,input().split())
arr=list(map(int,input().split()))

d=d%n 

start,end=0,n-1
while start<end:
    arr[start],arr[end]=arr[end],arr[start]
    start+=1 
    end-=1 
    
start,end=0,d-1
while start<end:
    arr[start],arr[end]=arr[end],arr[start]
    start+=1 
    end-=1 
    
start,end=d,n-1
while start<end:
    arr[start],arr[end]=arr[end],arr[start]
    start+=1 
    end-=1 
    
print(*arr)